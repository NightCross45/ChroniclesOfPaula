"code":230,"indent":0,"parameters":[15]},{"code":101,"indent":0,"parameters":["People3",6,0,2]},{"code":401,"indent":0,"parameters":["\"We caught you red-handed, thief. My name "]},{"code":401,"indent":0,"parameters":["is General Ale from the Queen's Knights.\""]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":4,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":4,"indent":null}]},{"code":101,"indent":0,"parameters":["People3",6,0,2]},{"code":401,"indent":0,"parameters":["\"Surrender immediately and your death will"]},{"code":401,"indent":0,"parameters":["be a swift one.\""]},{"code":213,"indent":0,"parameters":[5,6,true]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["\"Ugh...cornered. You can all rot in hell!\""]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":17,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":17,"indent":null}]},{"code":101,"indent":0,"parameters":["People3",6,0,2]},{"code":401,"indent":0,"parameters":["\"Knight, apprehend these criminals.\""]},{"code":101,"indent":0,"parameters":["People3",7,0,2]},{"code":401,"indent":0,"parameters":["\"At once, General Ale.\""]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":4,"indent":null},{"code":4,"indent":null},{"code":3,"indent":null},{"code":19,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":4,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":4,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":3,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":19,"indent":null}]},{"code":101,"indent":0,"parameters":["People3",7,0,2]},{"code":401,"indent":0,"parameters":["\"Do not make this difficult, sir.\""]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["\"Difficult? How was I supposed to feed"]},{"code":401,"indent":0,"parameters":["my son?!\""]},{"code":213,"indent":0,"parameters":[5,6,true]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameters":["\"I can't die, not here!\""]},{"code":212,"indent":0,"parameters":[1,6,true]},{"code":101,"indent":0,"parameters":["People3",7,0,2]},{"code":401,"indent":0,"parameters":["\"Ugh..\""]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":19,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":19,"indent":null}]},{"code":213,"indent":0,"parameters":[2,1,false]},{"code":213,"indent":0,"parameters":[3,1,false]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":2,"indent":null},{"code":2,"indent":null},{"code":2,"indent":null},{"code":19,"indent":null},{"code":15,"parameters":[5],"indent":null},{"code":41,"parameters":["Damage2",5],"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":2,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":2,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":2,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":19,"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":15,"parameters":[5],"indent":null}]},{"code":505,"indent":0,"parameters":[{"code":41,"parameters":["Damage2",5],"indent":null}]},{"code":101,"indent":0,"parameters":["People3",6,0,2]},{"code":401,"indent":0,"parameters":["\"You're a piece of filth for killing one"]},{"code":401,"indent":0,"parameters":["of my men. I will cut you in half.\""]},{"code":205,"indent":0,"parameters":[3,{"list":[{"code":4,"indent":null},{"code":0}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":4,"indent":null}]},{"code":212,"indent":0,"parameters":[5,6,true]},{"code":101,"indent":0,"parameters":["People4",4,0,2]},{"code":401,"indent":0,"parameter